project(DcpsFooTest5Publisher): dcpsexe_with_tcp, dcps_udp, dcps_multicast, dcps_rtps_udp {
  exename   = publisher
  libpaths += ../FooType5
  libs     +=  DcpsFooType5
  after    += DcpsFooType5

  Source_Files {
    publisher.cpp
    Writer.cpp
    common.cpp
    InstanceDataMap.cpp
  }
}


