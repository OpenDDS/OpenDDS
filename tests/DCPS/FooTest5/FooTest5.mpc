  exename   = subscriber
  libpaths += ../FooType5
  libs     +=  DcpsFooType5
  after    += DcpsFooType5

  Source_Files {
    subscriber.cpp
    DataReaderListener.cpp
    common.cpp
    InstanceDataMap.cpp
  }
}

