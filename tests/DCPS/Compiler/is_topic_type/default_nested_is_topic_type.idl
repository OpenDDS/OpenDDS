@default_nested
module DefaultNestedModule {
  struct NonAnnotatedStruct {
    long value;
  };

  @topic
  struct TopicStruct {
    long value;
  };

  @nested
  struct NestedStruct {
    long value;
  };

  @nested(TRUE)
  struct NestedTrueStruct {
    long value;
  };

  @nested(FALSE)
  struct NestedFalseStruct {
    long value;
  };
};

