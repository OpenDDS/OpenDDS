module Xyz {
  // Example user defined "Foo" structure
  @topic
  struct Foo {
    @key long  a_long_value;  // the instance key
    long  handle_value;
    long  sample_sequence;
    long  writer_id;
  };
};

//Need to verify that we can have a "Foo" (or in this case, a "Bar") outside
//of a module in IDL.
@topic
struct Bar {
  unsigned long data;
};

// Verify forward declared types
struct AA;
typedef sequence<AA> AASeq;

struct BB
{
  AASeq aa_;
};
typedef sequence<BB> BBSeq;

struct AA
{
  BBSeq bb_;
};
