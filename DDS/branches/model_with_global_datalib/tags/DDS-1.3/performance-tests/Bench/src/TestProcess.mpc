project(BenchIdl): dcps {
  // This project ensures the common components get built first.

  TypeSupport_Files {
    Test.idl
  }

  custom_only = 1
}

project(BenchProcess) : dcpsexe_with_unreliable_dgram, dcps_reliable_multicast, dcps_tcp, svc_utils {
  exename = testprocess
  exeout  = ../bin
  after  += BenchIdl

  TypeSupport_Files {
    Test.idl
  }

  Source_Files {
    Exponential.cpp
    Gaussian.cpp
    EntityProfiles.cpp
    Options.cpp
    Publication.cpp
    Subscription.cpp
    DataReaderListener.cpp
    Process.cpp
    testprocess.cpp
  }
}

