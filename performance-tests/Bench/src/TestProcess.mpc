project(BenchIdl): dcps {
  // This project ensures the common components get built first.

  TypeSupport_Files {
    Test.idl
  }

  custom_only = 1
}

project(BenchProcess) : dcpsexe, dcps_tcp, dcps_udp, dcps_multicast, dcps_rtps_udp, svc_utils {
  exename = testprocess
  exeout  = ../bin
  after  += BenchIdl

  TypeSupport_Files {
    Test.idl
  }

  Header_Files {
    Shutdown.h
  }

  Source_Files {
    StatisticalValue.cpp
    EntityProfiles.cpp
    Options.cpp
    Publication.cpp
    Subscription.cpp
    DataReaderListener.cpp
    Process.cpp
    testprocess.cpp
  }
}

