{
  "create_time": { "sec": -2, "nsec": 0 },
  "enable_time": { "sec": -2, "nsec": 0 },
  "start_time": { "sec": -2, "nsec": 0},
  "stop_time": { "sec": -80, "nsec":0 },
  "destruction_time": { "sec": -2, "nsec": 0 },

  "properties": [],

  "process": {
    "config_sections": [
      {
        "name": "common",
        "properties": [
          { "name": "DCPSSecurity",
            "value": 0
          },
          { "name": "DCPSDebugLevel",
            "value": 0
          }
        ]
      },
      {
        "name": "transport/rtps_transport",
        "properties": [
          { "name": "transport_type",
            "value": "rtps_udp"
          }
        ]
      }
    ],
    "discoveries": [
      {
        "name": "tagged_test_rtps",
        "type": "rtps",
        "domain": 7
      }
    ],
    "instances": [
      {
        "name": "rtps_instance_01",
        "type": "rtps_udp",
        "domain": 7
      }
    ],
    "participants": [
      {
        "name": "participant_01",
        "domain": 7,
        "transport_config_name": "rtps_instance_01",

        "qos": { "entity_factory": { "autoenable_created_entities": false } },
        "qos_mask": { "entity_factory": { "has_autoenable_created_entities": false } },

        "topics": [
          {
            "name": "ProcessedDataTopic",
            "type_name": "Bench::Data"
          },
          {
            "name": "ControlDataTopic",
            "type_name": "Bench::Data"
          }
        ],
        "publishers": [
          {
            "name": "publisher_01",
            "datawriters": [
              {
                "name": "datawriter_control_data",
                "topic_name": "ControlDataTopic",
                "listener_type_name": "bench_dwl",
                "listener_status_mask": 4294967295,
                "qos": { "reliability": { "kind": "RELIABLE_RELIABILITY_QOS" },
                         "history": { "depth": 10 }
                       },
                "qos_mask": { "reliability": { "has_kind": true },
                              "history": { "has_depth": true }
                            }
              }
            ]
          }
        ],
        "subscribers": [
          {
            "name": "subscriber_01",
            "datareaders": [
              {
                "name": "datareader_processed_data",
                "topic_name": "ProcessedDataTopic",
                "listener_type_name": "bench_drl",
                "listener_status_mask": 4294967295,
                "qos": { "reliability": { "kind": "RELIABLE_RELIABILITY_QOS" } },
                "qos_mask": { "reliability": { "has_kind": true } },
                "tags": [ "continuous", "processed" ]
              }
            ]
          }
        ]
      }
    ]
  },

  "actions": [
    {
      "name": "Write control data",
      "type": "write",
      "writers": [ "datawriter_control_data" ],
      "params": [
        {
          "name": "data_buffer_bytes",
          "value": { "_d": "PVK_ULL", "ull_prop": 128 }
        },
        {
          "name": "write_frequency",
          "value": { "_d": "PVK_DOUBLE", "double_prop": 0.5 }
        }
      ]
    }
  ],

  "action_reports": [],

  "wait_for_discovery": true,
  "wait_for_discovery_seconds": 10
}
